{
  "function": {
    "todoList": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "api": {
    "apiTodoList": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "todoList",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "apiTodoListGraph": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 365,
              "description": "graphQL todo list"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    }
  }
}